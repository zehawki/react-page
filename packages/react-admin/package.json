  "peerDependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "react-final-form": "*",
    "react": ">= 16.14",
    "react-dom": ">= 16.14",
    "react-admin": "^3.0.0",
    "uniforms": "*"
  },
  "dependencies": {
    "@react-page/editor": "0.0.0"
  },
  "devDependencies": {
    "react-admin": "^3.12.2",
    "uniforms": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
